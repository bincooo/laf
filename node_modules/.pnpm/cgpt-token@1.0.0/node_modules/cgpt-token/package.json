{
  "name": "cgpt-token",
  "version": "1.0.0",
  "description": "OpenAI Authentication Library for ChatGPT.",
  "type": "module",
  "source": "./src/index.ts",
  "types": "./build/index.d.ts",
  "files": [
    "build"
  ],
  "exports": {
    ".": {
      "import": "./build/index.js",
      "types": "./build/index.d.ts",
      "default": "./build/index.js"
    }
  },
  "engines": {
    "node": ">=14.18.0 <15.0.0 || >=16.0.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
