    "prebuild": "npm run clean",
    "predev": "npm run clean"
  },
  "keywords": [
    "openai",
    "auth",
    "chatgpt",
    "gpt3",
    "gpt-3"
  ],
  "author": "bincooo",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bincooo/openai-token.git"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^1.3.3",
    "axios-cookiejar-support": "^4.0.6",
    "http-cookie-agent": "^5.0.2",
    "http-proxy-agent": "^5.0.0",
    "https-proxy-agent": "^5.0.1",
    "tough-cookie": "^4.1.2"
  },
  "devDependencies": {
    "del-cli": "^5.0.0",
    "tsup": "^6.6.3",
    "@types/node": "14.18.36",
    "typescript": "^4.9.5"
  },
  "bugs": {
    "url": "https://github.com/bincooo/openai-token/issues"
  },
  "homepage": "https://github.com/bincooo/openai-token#readme",
